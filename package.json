{
    "name": "selenium-mocha-digital-skola",
    "version": "1.0.0",
    "description": "Selenium WebDriver tests using Mocha with Mochawesome reports",
    "scripts": {
        "test": "mocha tests/sesi11.js --reporter mochawesome --recursive --timeout 60000",
        "report": "mocha tests/sesi11.js --reporter mochawesome --recursive --timeout 60000 --reporter-options reportDir=mochawesome-report"
    },
    "dependencies": {
        "selenium-webdriver": "^4.17.0"
    },
    "devDependencies": {
        "exceljs": "^4.4.0",
        "mocha": "^10.4.0",
        "mocha-simple-html-reporter": "^2.0.0",
        "mochawesome": "^7.1.3",
        "pixelmatch": "^7.1.0",
        "pngjs": "^7.0.0",
        "selenium-webdriver": "^4.17.0"
    }
}
